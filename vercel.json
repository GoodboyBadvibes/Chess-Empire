{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "outputDirectory": "dist/public",
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/index.cjs" },
    { "src": "/(.*)", "dest": "/dist/public/$1" },
    { "src": "/(.*)", "dest": "/dist/index.cjs" }
  ]
}